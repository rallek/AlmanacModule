{# Purpose of this template: Display dates within an external context #}
{% for date in items %}
    <h3>{{ date|rkalmanacmodule_formattedTitle }}</h3>
    <p><a href="{{ path('rkalmanacmodule_date_display', {'id': date.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
